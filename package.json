{
  "name": "mojitonft",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojitoinc/mixers.git"
  },
  "scripts": {
    "init": "yarn global add gulp webpack-cli && yarn install && yarn run clean && yarn run bootstrap && yarn run build",
    "start": "yarn run dev",
    "dev": "yarn --cwd app build && dumi dev",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean --yes",
    "build": "lerna run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ignore-pattern **/__tests__/* --ignore-pattern **/demo/* \"packages/*/src/**/*.{ts,tsx}\"",
    "build:doc": "dumi build",
    "pub:doc": "yarn run build:doc",
    "pub": "yarn run build && lerna publish",
    "pub:beta": "yarn run build && lerna publish --dist-tag beta"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "del": "^5.1.0",
    "dumi": "^1.1.7",
    "enzyme": "^3.10.0",
    "eslint": "^7.2.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.0.1",
    "gray-matter": "^4.0.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-extended": "^2.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-localstorage-mock": "^2.4.18",
    "lerna": "4.x",
    "mockdate": "^3.0.2",
    "mockjs": "^1.1.0",
    "msw": "^0.41.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-test-renderer": "^18.1.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.10.1",
    "@types/auth0": "^2.35.0",
    "@types/react-query": "^1.2.9",
    "auth0-react": "^3.1.0",
    "graphql-request": "^4.3.0",
    "moment": "^2.29.3",
    "react-query": "^3.39.1"
  }
}
